<li class="event [[+eo:ne=`0`:then=`alt`]]">
    <a aria-label="[[+start:date=`%b %e`]] [[+infoURL]]" href="[[+infoURL]]" rel="nofollow" class="gccalevent" data-index="[[+id]]">
        <div class="date">[[+start:date=`<span>%b</span><br/>%e`]]</div>
        <div class="details"><strong>[[+title]]</strong><br />[[+span]] &bull; [[+ad:ne=`1`:then=`[[+start:date=`%l:%M %P`]]`:else=`All Day`]]</div>
        <div class="expand icon-expand"></div>
    </a>
</li>